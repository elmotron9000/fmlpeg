{
  "name": "@elmotron9000/fmlpeg",
  "version": "0.1.9",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "debug": "TS_NODE_FILES=true nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "pretest": "rm -rf ./test/resources/output/*.{mp4,srt}",
    "test": "jest",
    "coverage": "jest --coverage",
    "publisher": "rm -rf dist/; npm run build && npm publish --access=public"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmotron9000/fmlpeg.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "assets",
    "dist",
    "!dist/**/*.map"
  ],
  "author": "Tom Carrio <tom@carrio.dev>",
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.0",
    "@types/prettier": "^1.16.4",
    "@types/temp": "^0.8.34",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "temp": "^0.9.1"
  }
}
